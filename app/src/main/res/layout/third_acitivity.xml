<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".ThirdActivity">


    <TextView
        android:id="@+id/nomeLabelTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="1dp"
        android:text="Nome"
        android:textSize="15dp"
        android:textStyle="bold"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toTopOf="@+id/nomeTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mapView" />

    <TextView
        android:id="@+id/enderecoLabelTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="3dp"
        android:text="Endereco"
        android:textSize="15dp"
        android:textStyle="bold"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toTopOf="@+id/enderecoTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nomeTextView" />

    <TextView
        android:id="@+id/enderecoTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="21dp"
        android:textSize="15dp"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toTopOf="@+id/filaTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enderecoLabelTextView" />

    <TextView
        android:id="@+id/filaLabelTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="1dp"
        android:text="Fila"
        android:textSize="15dp"
        android:textStyle="bold"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enderecoTextView" />

    <TextView
        android:id="@+id/filaTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="23dp"
        android:textSize="15dp"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toTopOf="@+id/tempoTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/enderecoTextView" />

    <TextView
        android:id="@+id/tempoLabelTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="2dp"
        android:text="Tempo de espera"
        android:textSize="15dp"
        android:textStyle="bold"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/filaTextView" />

    <TextView
        android:id="@+id/tempoTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:textSize="15dp"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toTopOf="@+id/especialidadeLabelTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/filaTextView" />

    <TextView
        android:id="@+id/especialidadeLabelTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:text="Especialidades "
        android:textSize="15dp"
        android:textStyle="bold"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toTopOf="@+id/especialidadeTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/tempoTextView" />

    <TextView
        android:id="@+id/especialidadeTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="69dp"
        android:textSize="15dp"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/especialidadeLabelTextView" />

    <TextView
        android:id="@+id/nomeTextView"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginBottom="2dp"
        android:textSize="15dp"
        app:fontFamily="@font/roboto_slab_light"
        app:layout_constraintBottom_toTopOf="@+id/enderecoLabelTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/nomeLabelTextView" />

    <com.google.android.gms.maps.MapView
        android:id="@+id/mapView"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginTop="34dp"
        android:layout_marginBottom="27dp"
        app:layout_constraintBottom_toTopOf="@+id/nomeLabelTextView"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


</androidx.constraintlayout.widget.ConstraintLayout>